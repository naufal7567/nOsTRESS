<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment_menu.PsikologFragment"
    android:background="@drawable/bg_app">


    <TextView
        android:id="@+id/tv_title_list_psikolog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:shadowColor="#000000"
        android:shadowDx="1.5"
        android:shadowDy="1.3"
        android:shadowRadius="1.5"
        android:textAllCaps="true"
        android:gravity="center"
        android:fontFamily="@font/poppins_bold"
        android:text="List kontak psikolog"
        android:layout_centerHorizontal="true"
        android:textColor="@android:color/holo_orange_light"
        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/ivPsikolog1"
        android:layout_width="150dp"
        android:layout_height="140dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="56dp"
        android:focusable="true"
        android:src="@drawable/ibu_gilberta"
        app:civ_border_color="@android:color/holo_orange_light"
        app:civ_border_width="4dp"
        app:civ_shadow="true"
        app:civ_shadow_color="#0099FF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_list_psikolog"
        tools:ignore="MissingConstraints" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/ivPsikolog2"
        android:layout_width="150dp"
        android:layout_height="140dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:focusable="true"
        android:src="@drawable/titin"
        app:civ_border_color="@android:color/holo_orange_light"
        app:civ_border_width="4dp"
        app:civ_shadow="true"
        app:civ_shadow_color="#0099FF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivPsikolog1"
        tools:ignore="MissingConstraints" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/ivPsikolog3"
        android:layout_width="150dp"
        android:layout_height="140dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:focusable="true"
        android:src="@drawable/budi"
        app:civ_border_color="@android:color/holo_orange_light"
        app:civ_border_width="4dp"
        app:civ_shadow="true"
        app:civ_shadow_color="#0099FF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivPsikolog2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvNamaPsikolog1"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Gilberta P. Mahanani, \n M.Psi, Psikolog"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog1"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_list_psikolog" />

    <TextView
        android:id="@+id/tvDetailPsikolog1"
        android:layout_width="224dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Psikolog Klinis RSUP Dr. Wahidin Sudirohusodo Makassar"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog1"
        app:layout_constraintTop_toBottomOf="@+id/tvNamaPsikolog1" />

    <TextView
        android:id="@+id/tvNamaPsikolog2"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Titin Florentina P., M. Psi., Psikolog"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog2"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/tvDetailPsikolog2"
        android:layout_width="217dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Dosen Psikolog Universitas Bosowa Makassar"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog2"
        app:layout_constraintTop_toBottomOf="@+id/tvNamaPsikolog2" />

    <TextView
        android:id="@+id/tvNamaPsikolog3"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Andi Budhy Rakhmat, M.Psi., Psikolog"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/tvDetailPsikolog3"
        android:layout_width="214dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Dosen Psikolog Univeristas Bosowa Makassar"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog3"
        app:layout_constraintTop_toBottomOf="@+id/tvNamaPsikolog3" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="deargilby@gmail.com"
        android:autoLink="email"
        android:textColor="@android:color/holo_orange_light"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog1"
        app:layout_constraintTop_toBottomOf="@+id/tvDetailPsikolog1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:autoLink="email"
        android:text="budhyapuak@gmail.com"
        android:textColor="@android:color/holo_orange_light"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog3"
        app:layout_constraintTop_toBottomOf="@+id/tvDetailPsikolog3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:autoLink="email"
        android:text="titinflorentina77@gmail.com"
        android:textColor="@android:color/holo_orange_light"
        app:layout_constraintStart_toEndOf="@+id/ivPsikolog2"
        app:layout_constraintTop_toBottomOf="@+id/tvDetailPsikolog2" />


</androidx.constraintlayout.widget.ConstraintLayout>